@model LancheListViewModel
<div>
    <h1>@Model.CategoriaAtual</h1>
</div>
<hr />
<h5>@ViewBag.Total @ViewBag.TotalLanches</h5>
<hr />
<div class="container">
    <div class="row">
        @foreach (var lanche in Model.Lanches)
        {
            <partial name="_LanchesResumo" for="@lanche" />
        }
    </div>
</div>